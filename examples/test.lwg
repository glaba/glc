trait T1 {
    properties {
        var_T1 : bool
    }

    always {}
}

trait T2 {
    properties {
        var_T2: bool
    }

    always {}
}

trait Test {
    properties {
        var1 : int<0, 5>
    }

    always {
        for a in range of this with trait T1 {
            for b in range of a with trait T2 {
                for c in range of b with trait Test {
                    a.var_T1 = 5;
                    b.var_T2 = 5;
                    c.var1 = 5;
                }
            }
        }
    }
}

unit test : T2(var_T2 = true), Test(var1 = 3), T1;